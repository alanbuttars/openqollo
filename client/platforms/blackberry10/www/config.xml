<?xml version='1.0' encoding='utf-8'?>
<widget id="com.alanbuttars.qollo" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:rim="http://www.blackberry.com/ns/widgets">
    <name>OpenQollo</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <feature name="com.blackberry.utils" />
    <feature name="com.blackberry.pim.lib" />
    <feature name="Contacts" value="Contacts" />
    <rim:permissions>
        <rim:permit>access_pimdomain_contacts</rim:permit>
        <rim:permit>access_shared</rim:permit>
        <rim:permit>use_camera</rim:permit>
    </rim:permissions>
    <feature name="Camera" value="Camera" />
    <feature name="File" value="File" />
    <feature name="FileTransfer" value="FileTransfer" />
</widget>
